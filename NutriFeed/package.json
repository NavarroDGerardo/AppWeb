  "description": "proyecto de la materia de apliaciones web",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-index": "node-sass --include-path scss scss/main.scss public/scss/main.css",
    "watch-index": "nodemon -e scss -x \"npm run build-index\"",
    "build-login": "node-sass --include-path scss scss/login/login.scss public/scss/login.css",
    "watch-login": "nodemon -e scss -x \"npm run build-login\"",
    "build-nutri": "node-sass --include-path scss scss/nutri.scss public/scss/nutri.css",
    "watch-nutri": "nodemon -e scss -x \"npm run build-nutri\"",
    "css:minify": "cleancss --level 1 --source-map --source-map-inline-sources --output public/css/main.min.css public/css/main.css ",
    "css:lint": "stylelint --config .stylelintrc --syntax scss \"scss/**/*.scss\""

  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NavarroDGerardo/AppWeb.git"
  },
